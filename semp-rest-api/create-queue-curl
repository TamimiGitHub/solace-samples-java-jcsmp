curl http://localhost:8080/SEMP/v2/config/msgVpns/default/queues \
  -X POST \
  -u admin:admin \
  -H "Content-type:application/json" \
  -d '{ "queueName": "q_samples",
        "accessType": "exclusive",
        "maxMsgSpoolUsage": 100,
        "permission": "consume",
        "ingressEnabled": true,
        "egressEnabled": true }'
